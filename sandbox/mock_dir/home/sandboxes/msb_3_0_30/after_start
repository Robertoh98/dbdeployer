# Generated by dbdeployer 1.73.0 using after_start on Thu Jun 26 18:51:24 -03 2025
source /Users/robertogarciadebem/Source codes/dbdeployer/sandbox/mock_dir/home/sandboxes/msb_3_0_30/sb_include

# Removes the scripts that we know for sure won't be used with TiDB
#
scripts_to_delete=(add_option show_binlog show_relaylog replicate_from connection.sql)
for script in ${scripts_to_delete[*]}
do
    if [ -f $SBDIR/$script ]
    then
        rm -f $SBDIR/$script
    fi
done
exit 0
